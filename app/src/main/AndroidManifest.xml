<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="volume.sound.booster"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-feature android:name="android.hardware.location" android:required="false" />
    <uses-feature android:name="android.hardware.location.network" android:required="false" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/icons"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="volume.sound.booster.StartActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="volume.sound.booster.MainActivity"
                  android:label="@string/app_name">

        </activity>
        <service android:name="volume.sound.booster.SoundBoosterService" android:enabled="true" android:exported="false">
            <intent-filter>
                <action android:name="com.kevinboone.android_simple_eq.VolumeBoosterService.SERVICE" />
            </intent-filter>
        </service>
    </application>



</manifest> 
